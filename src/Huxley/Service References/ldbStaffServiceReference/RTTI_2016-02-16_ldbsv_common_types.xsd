<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://thalesgroup.com/RTTI/2016-02-16/ldbsv/commontypes" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://thalesgroup.com/RTTI/2016-02-16/ldbsv/commontypes" id="RTTI_2016-02-16_ldbsv_common_types" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="RSIDType">
    <xs:annotation>
      <xs:documentation>A Retail Service Identifier</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="6" />
      <xs:maxLength value="8" />
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ServiceIDType">
    <xs:annotation>
      <xs:documentation>The ID of the service. Must be a valid UID, Headcode, or RSID. It must be a string of 4, 6 or 8 alphanumerics.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:minLength value="4" />
      <xs:maxLength value="8" />
    </xs:restriction>
  </xs:simpleType>
</xs:schema>